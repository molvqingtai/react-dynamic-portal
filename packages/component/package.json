{
  "name": "react-dynamic-portal",
  "version": "1.0.0",
  "description": "React Portal with dynamic mounting support",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsdown src/index.ts --dts --format esm --sourcemap --watch",
    "build": "tsdown src/index.ts --dts --format esm --sourcemap --minify --clean",
    "lint": "eslint --fix --cache",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "portal",
    "dynamic",
    "browser-extension",
    "content-script",
    "dom",
    "mutation-observer",
    "inject",
    "mount",
    "anchor",
    "reactdom",
    "createportal",
    "spa",
    "single-page-application",
    "web-extension",
    "chrome-extension",
    "firefox-extension",
    "dynamic-content",
    "dom-manipulation",
    "typescript"
  ],
  "author": "molvqingtai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/molvqingtai/react-dynamic-portal.git"
  },
  "bugs": {
    "url": "https://github.com/molvqingtai/react-dynamic-portal/issues"
  },
  "homepage": "https://github.com/molvqingtai/react-dynamic-portal#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20"
  },
  "publishConfig": {
    "access": "public"
  }
}
